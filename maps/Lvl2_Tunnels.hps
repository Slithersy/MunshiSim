	////////////////////////////
	// Update
	void Update(float afTimeStep)
	{
		tString sSimOver = GetSimEnd();
		array<tString> vSimOver = cString_Split(sSimOver, "_");
		if (vSimOver.size()>0)
		{
			bool bIsOver = cString_ToBool(vSimOver[0], false);
			tString sPathName = vSimOver[1];
            tString sFullName = vSimOver[1]+vSimOver[2]+"_"+vSimOver[3];
			
			if (bIsOver)
			{
				cLux_AddDebugMessage("Text over - "+sFullName);
				
				if (sPathName=="InteractCamera") {
                    Player_SetActive(true);
                } else if (sFullName=="RobotGirlEncounter3_1") {
                    Entity_SetActive("BlockBoxSpecial_3", false);
                    Entity_SetActive("DistortionSpecial_3", false);
                    bCanRun = true;
                    if (lIsInTryToRun==1) {
                        Trigger_TryToRun("", "", 1);
                    }
                } else if (sPathName=="RobotGirlEncounterRun") {
                    MakeCameraAnimation("CameraAnimGettingUp", true, false);
                    MunshiWalk(1, 5, 0.4f);
                    Map_AddTimer("Timer_CallStoodUpMessage", 2.0f, "Timer_CallStoodUpMessage");
                } else if (sFullName=="RobotGirlEncounter7_3") {
                    if (bHasStoodUp)
                        MunshiWalk(7,18, 0.25f);
                    else {
                        MunshiWalk(5,18, 0.25f);
                        Entity_SetActive("BlockBoxSpecial_3", false);
                        Entity_SetActive("DistortionSpecial_3", false);
                    }
                    
                    Entity_SetActive("BlockBoxSpecial_1", false);
                    Entity_SetActive("DistortionSpecial_1", false);
                    Entity_SetActive("BlockBoxSpecial_5", false);
                    Entity_SetActive("DistortionSpecial_5", false);
                    Entity_SetActive("BlockBoxSpecial_7", true);
                    Entity_SetActive("DistortionSpecial_7", true);
                    cStopMusic();
                    SetObjective("FollowRobotGirl", true);
                } else if (sFullName=="RobotGirlSewer1_3") {
                    MunshiWalk(20,24, 0.35f);
                    Entity_SetActive("BlockBoxSpecial_7", false);
                    Entity_SetActive("DistortionSpecial_7", false);
                    Entity_SetActive("BlockBoxSpecial_9", true);
                    Entity_SetActive("DistortionSpecial_9", true);
                } else if (sFullName=="RobotGirlSewer2_5") {
                    MunshiWalk(25,29, 0.35f);
                    Entity_SetActive("BlockBoxSpecial_9", false);
                    Entity_SetActive("DistortionSpecial_9", false);
                    Entity_SetActive("BlockBoxSpecial_10", true);
                    Entity_SetActive("DistortionSpecial_10", true);
                    Entity_SetActive("BlockBoxSpecial_11", true);
                    Entity_SetActive("DistortionSpecial_11", true);
                    //Entity_SetActive("BlockBoxSpecial_12", true);
                    //Entity_SetActive("DistortionSpecial_12", true);
                } else if (sFullName=="RobotGirlSewer7_5") {
                    EndLevel("Lvl3_MunshisHouse");
                }
            }
        }
        
		tString sSimUpdate = GetSimUpdate();
		array<tString> vSimUpdate = cString_Split(sSimUpdate, "_");
		if (vSimUpdate.size()>0)
		{
			bool bUpdated = cString_ToBool(vSimUpdate[0], false);
			tString sPathName = vSimUpdate[1];
            tString sFullName = vSimUpdate[1]+vSimUpdate[2]+"_"+vSimUpdate[3];

            if (bUpdated) {
                if (sFullName=="RobotGirlEncounter1_2") {
                    Entity_SetActive("robothead_real", false);
                } else if (sFullName=="RobotGirlEncounter2_4") {
                    if (bHasStoodUp)
                        ChangeMunshiAnim("RobotGirl_Encounter_11");
                    else ChangeMunshiAnim("RobotGirl_Encounter_6");
                } else if (sFullName=="RobotGirlEncounter2_5") {
                    if (bHasStoodUp)
                        ChangeMunshiAnim("RobotGirl_Encounter_12");
                    else ChangeMunshiAnim("RobotGirl_Encounter_7");
                } else if (sFullName=="RobotGirlEncounter7_1") {
                    if (bHasStoodUp)
                        ChangeMunshiAnim("RobotGirl_Encounter_11");
                    else ChangeMunshiAnim("RobotGirl_Encounter_9");
                } else if (sFullName=="RobotGirlSewer3_4") {
                    Entity_SetActive("hands_human_3", true);
                    Prop_MoveLinearTo("Box_Move_1","Box_Move_2",3,2,0.1f,false,"");
                    Prop_AlignRotation("Box_Move_1","Box_Move_2",0.3,1,0.4f,false,"");
                    Sound_CreateAtEntity("HitMetal", "physics/metal/pipe/pipe_impact", "oceanbottom_stairs_phi_walkway_phi_walkway_railing_loading_platform_3", 0, false);
                    Sound_CreateAtEntity("RobotBreathe", "NPC_Creatures/Vocalisations/InWorld/Robothead/erratic_breathing", "RobotGirl_Sewer_8", 0, false);
                } else if (sFullName=="RobotGirlSewer4_1") {
                    Entity_SetActive("hands_human_3", false);
                    Entity_SetActive("hands_human_4", true);
                    
                    cPlayMusic("music/HuniePop/bar_lounge.mp3", 0.4f, 10.0f, true);
                } else if (sFullName=="RobotGirlSewer4_2") {
                    Entity_SetActive("hands_human_4", false);
                } else if (sFullName=="RobotGirlSewer6_1") {
                    cScript_SetGlobalVarBool("Secret_RobotGirlComesIn", true);
                }
            }
        }
        
        if (Entity_IsActive("robothead_real")) {
            iLuxEntity@ pRobotGirl = cLux_GetCurrentMap().GetEntityByName("robothead_real", eLuxEntityType_LastEnum, "");
            pRobotGirl.FadeEffectBaseColor(cColor(1.5f, 1.0f), 1.0f);
        } else if (Entity_IsActive("David_WalkLeft_18") && bReachedCircle) {
            Entity_SetActive("Trigger_ReachedCircle", false);
            bReachedCircle = false;
            bReachedCircleCan = false;
            
            Entity_SetActive("BlockBoxSpecial_8", true);
            Entity_SetActive("DistortionSpecial_8", true);
            MakeMessage("RobotGirlSewer", 1, 1);
        } else if (Entity_IsActive("David_WalkRight_24") && bReachedFirst) {
            bReachedFirst = false;
            bReachedFirstCan = false;
            
            MakeMessage("RobotGirlSewer", 2, 1);
        } else if (Entity_IsActive("David_WalkRight_29") && bReachedSecond) {
            bReachedSecond = false;
            bReachedSecondCan = false;
            
            Entity_SetActive("BlockBoxSpecial_12", true);
            Entity_SetActive("DistortionSpecial_12", true);
            MakeMessage("RobotGirlSewer", 3, 1);
        }
    }

