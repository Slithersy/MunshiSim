		if (bNumPad9) 	{ Keypad_EnterDigit("9", asEntityName); }
		if (bNumPad0) 	{ Keypad_EnterDigit("0", asEntityName); }
		if (bNumPadAst)	{ Keypad_EnterDigit("*", asEntityName); }
		if (bNumPadHash){ Keypad_EnterDigit("#", asEntityName); }
		
	}
	
