    ////Mug protection////
    int lTimesThrownCup = 0;
    bool CupTeleportBack(const tString &in asParent, const tString &in asChild, int alState)
    {
        array<tString> SplitName = cString_Split(asChild, "_");
        if (SplitName.size()>0)
        {
            //mug_coffee_2
            int sNumber = cString_ToInt(SplitName[2], 1);
            if (asParent == "Area_CupTeleport_1")
            {
                Map_RemoveTimer("MugThrow_"+sNumber);
                MugThrow("mug_"+sNumber);
            } else if (alState == -1) {
                Map_AddTimer("MugThrow_"+sNumber, 0.5f, "MugThrow");
                
                lTimesThrownCup++;
                if (lTimesThrownCup > 5) {
                    Hint_ShowHint("DateSim_Hints", "StopThrowingMugs", false, 2.0f);
                }
            }
        }
        
        return true;
    }
    
    void MugThrow(const tString &in asTimer) {
        array<tString> SplitName = cString_Split(asTimer, "_");
        if (SplitName.size()>0)
        {
            int sNumber = cString_ToInt(SplitName[1], 1);
            
            iLuxEntity@ pMugEntity = cLux_GetCurrentMap().GetEntityByName("mug_coffee_"+sNumber, eLuxEntityType_LastEnum, "");
            if (pMugEntity is null)
            {
                Error("Could not find the target entity mug_coffee_2");
                return;
            }
            
            if (sNumber == 1)
                pMugEntity.SetPosition(cVector3f(-68.5, 3.90, 100));
            else if (sNumber == 2) pMugEntity.SetPosition(cVector3f(-68.706, 3.90, 99.437));
            else {
                Sound_PlayGui("MunshiSim/Game/fart_reverb", 0.8f);
                Entity_Destroy(pMugEntity.GetName());
                ChangeReputation(-10);
            }
            
            pMugEntity.SetIsInteractedWith(false);
            iPhysicsBody @pBody = pMugEntity.GetMainBody();
            pBody.SetLinearVelocity(cVector3f(0,0,0));
            pBody.SetAngularVelocity(cVector3f(0,0,0));
        }
    }
    
    
    ////Munshi////
    bool bHasUsedMachine = false;
    
    void MunshiAtCoffee(const tString &in asTimer) {
        ChangeMunshiAnim("David_CoffeeShow_1");
